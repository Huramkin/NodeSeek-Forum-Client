{
  "name": "nodeseek-client",
  "version": "0.1.0",
  "description": "NodeSeek DeepFlood forum desktop client built with Electron and React",
  "author": "NodeSeek Contributors",
  "license": "MIT",
  "main": "dist/main/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:main\" \"npm:dev:preload\" \"npm:dev:electron\"",
    "dev:renderer": "vite dev",
    "dev:main": "tsc -w -p tsconfig.main.json",
    "dev:preload": "tsc -w -p tsconfig.preload.json",
    "dev:electron": "wait-on dist/main/main.js dist/preload/index.js http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build:renderer": "vite build",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "package": "npm run build && electron-builder"
  },
  "dependencies": {
    "electron-store": "^9.0.0",
    "keytar": "^7.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.11",
    "webdav": "^5.6.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.33",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "electron-builder": "^25.1.8",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=20"
  }
}
